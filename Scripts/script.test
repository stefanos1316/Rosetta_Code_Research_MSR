#!/bin/bash


echo "Making energy delay product results"

#Reading from two identical files
while read line; do
  	if ! read -u 3 line2
  	then
    		break	
	fi

	IFS=', ' read -r -a array <<< "$line"
	IFS=', ' read -r -a array <<< "$line2"

	for element in ${!line[*]}; do
		echo "${line[$element]} and ${line2[$element]}"
	done

done < $1 3<$2


